    "SOURCE":"@AccessControl.authorizationCheck: #NOT_REQUIRED\r\n@EndUserText.label: 'DBS Limit G端ncelleme'\r\n@Metadata.ignorePropagatedAnnotations: true\r\ndefine root view entity YDBS_DDL_I_LIMIT\r\n  as select from    ydbs_t_subsmap\r\n    inner join      I_Customer              as kna1  on kna1.Customer = ydbs_t_subsmap.customer\r\n    left outer join YDBS_DDL_I_LATEST_LIMIT as limit on  limit.Companycode    = ydbs_t_subsmap.companycode\r\n                                                     and limit.Bankinternalid = ydbs_t_subsmap.bankinternalid\r\n                                                     and limit.Customer       = ydbs_t_subsmap.customer\r\n{\r\n  key ydbs_t_subsmap.companycode       as Companycode,\r\n  key ydbs_t_subsmap.bankinternalid    as Bankinternalid,\r\n  key ydbs_t_subsmap.customer          as Customer,\r\n      kna1.CustomerName                as CustomerName,\r\n      kna1.CustomerFullName            as CustomerFullname,\r\n      ydbs_t_subsmap.subscriber_number as SubscriberNumber,\r\n      kna1.TaxNumber1                  as TaxNumber1,\r\n      kna1.TaxNumber2                  as TaxNumber2,\r\n      limit.Currency,\r\n      limit.LimitDate,\r\n      limit.LimitTime,\r\n      @Semantics.amount.currencyCode: 'Currency'\r\n      limit.TotalLimit,\r\n      @Semantics.amount.currencyCode: 'Currency'\r\n      limit.AvailableLimit,\r\n      @Semantics.amount.currencyCode: 'Currency'\r\n      limit.Risk,\r\n      @Semantics.amount.currencyCode: 'Currency'\r\n      limit.MaturityAmount,\r\n      limit.MaturityInvoiceCount,\r\n      @Semantics.amount.currencyCode: 'Currency'\r\n      limit.OverLimit\r\n\r\n}\r\n/*+[internal] {\n\"BASEINFO\":\n{\n\"FROM\":\n[\n\"I_CUSTOMER\",\n\"YDBS_DDL_I_LATEST_LIMIT\",\n\"YDBS_T_SUBSMAP\"\n],\n\"ASSOCIATED\":\n[],\n\"BASE\":\n[],\n\"ANNO_REF\":\n[],\n\"SCALAR_FUNCTION\":\n[],\n\"VERSION\":0,\n\"ANNOREF_EVALUATION_ERROR\":\"\"\n}\n}*/",
